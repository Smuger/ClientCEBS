<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="cruiseexcursionbookingsystem.controllers.mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kwietniewski.clientcebs.BookingController">
    <stylesheets>
        <URL value="@/styles/booking.css" />
    </stylesheets>
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <ListView fx:id="listView" prefHeight="200.0" prefWidth="200.0" />
            <Button fx:id="refresh" mnemonicParsing="false" onAction="#refreshButton" text="Refresh" />
            <Button mnemonicParsing="false" onAction="#detailButton" text="Account details" />
            <Button mnemonicParsing="false" onAction="#backButton" text="Back" />
         </children>
      </VBox>
   </children>
</AnchorPane>
