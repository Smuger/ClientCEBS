<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" 
            styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" 
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kwietniewski.clientcebs.LoginController">
    <stylesheets>
        <URL value="@/styles/login.css" />
    </stylesheets>
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <TextField fx:id="email" promptText="email" />
            <TextField fx:id="password" promptText="Password" />
            <Text fx:id="error" strokeType="OUTSIDE" strokeWidth="0.0" />
            <Button mnemonicParsing="false" onAction="#loginButton" text="Login" />
            <Button mnemonicParsing="false" onAction="#registerButton" text="Register" />
         </children>
      </VBox>
   </children>
</AnchorPane>
