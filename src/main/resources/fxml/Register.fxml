<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kwietniewski.clientcebs.RegisterController">
    <stylesheets>
        <URL value="@/styles/register.css" />
    </stylesheets>
   <children>
      <VBox prefHeight="393.0" prefWidth="239.0">
         <children>
            <TextField fx:id="name" promptText="Full name" />
            <Label text="( Min 3 / Max 10 characters)" />
            <TextField fx:id="email" promptText="Email" />
            <Label text="( Min 5 / Max 30 characters)" />
            <TextField fx:id="passwd1" promptText="Password" />
            <Label text="( Min 3 / Max 30 characters)" />
            <TextField fx:id="passwd2" promptText="Password" />
            <Label text="( Min 3 / Max 30 characters)" />
            <CheckBox mnemonicParsing="false" onAction="#adminHandler" text="Are you a trip provider?" />
            <Text fx:id="error" strokeType="OUTSIDE" strokeWidth="0.0" />
            <Button mnemonicParsing="false" onAction="#registerButton" text="Register" />
            <Button mnemonicParsing="false" onAction="#backButton" text="back" />
         </children>
      </VBox>
   </children>
</AnchorPane>
